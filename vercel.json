{
    "redirects": [
        {
            "source": "https://wondrlink.vercel.app",
            "destination": "https://wondrlink.com",
            "permanent": true
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Robots-Tag",
                    "value": "noindex, nofollow"
                },
                {
                    "key": "Cache-Control",
                    "value": "no-cache"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/api/:match*",
            "destination": "https://wondrlink-back.vercel.app/api/:match*"
        },
        {
            "source": "/(.*)",
            "destination": "https://wondrlink.vercel.app/$1"
        }
    ]
}